{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "tikio",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "types": [],
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "packages/**/tests/**",
    "packages/**/dist/**"
  ],
  "references": [
    {
      "path": "./packages/experiment"
    },
    {
      "path": "./packages/frontend"
    }
  ]
}
