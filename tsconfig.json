{
  "extends": "./tsconfig.build.json",
  "compilerOptions": {
    "paths": {
      "@tikio/app": [
        "packages/app/src"
      ],
      "@tikio/es4x": [
        "packages/es4x/src"
      ],
      "@tikio/wire": [
        "packages/wire/src"
      ],
      "@tikio/*": [
        "packages/*/src"
      ]
    }
  },
  "references": [
    {
      "path": "./packages/app"
    },
    {
      "path": "./packages/es4x"
    },
    {
      "path": "./packages/wire"
    }
  ]
}
